{
    "env": {
        "es6": true
    },
    "extends": ["plugin:vue/base"],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "semi": [
            "error",
            "never"
        ],
        "quotes": [
            "error",
            "single"
        ]
    }
}